[
	{
		"path": "news",
		"controller": "controllers\\news@index",
		"method":"get"
	},
	{
		"path": ["news", "view", {"type":"dynamic", "name":"id", "regex":"/^(\\d+)$/"}],
		"controller": "controllers\\news@view"
	},
	{
		"path": ["news", "archive", {"type":"dynamic", "name":"year", "regex":"/^(\\d){4}$/"}, {"type":"dynamic", "name":"month", "regex":"/^(\\d+){2}$/"}],
		"controller": "controllers\\news@archive"
	},
	{
		"path": ["news", "author", {"type":"dynamic", "name":"id", "regex":"/^(\\d+)$/"}],
		"controller": "controllers\\news@author"
	},
	{
		"path": ["news", "rss"],
		"controller": "controllers\\rss@build"
	},
	{
		"path": ["userpanel", "news", "published"],
		"controller": "controllers\\panel\\news@published",
		"method": "get"
	},
	{
		"path": ["userpanel", "news", "unpublished"],
		"controller": "controllers\\panel\\news@unpublished",
		"method": "get"
	},
	{
		"path": ["userpanel", "news", "comments"],
		"controller": "controllers\\panel\\news@comments",
		"method": "get"
	},
	{
		"path": ["userpanel", "news", "comments", {"type":"dynamic", "name":"id", "regex":"/^(\\d+)$/"}],
		"controller": "controllers\\panel\\news@comments",
		"method": "get"
	}

]
