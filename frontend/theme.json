{
	"name" : "clipone",
	"title":"Clip One",
	"version" : "1.0.0",
	"author" : {
		"name" : "Jeyserver",
		"website" : "http://www.jeyserver.com/"
	},
	"autoload": {
		"directories": ["listeners", "views"]
	},
	"languages":{
		"fa_IR" : "langs/fa_IR.json"
	},
	"assets":[
		{"type":"package", "name":"jquery", "version": "^3.2.1"},
		{"type":"package", "name":"jquery-ui", "version": "^1.12.1"},
		{"type":"package", "name":"bootstrap", "version": "^3.3.7"},
		{"type":"package", "name":"webuilder", "version": "1.1.3"},
		{"type":"package", "name":"jquery-inputlimiter", "version": "^1.3.2"},
		{"type":"package", "name":"jquery.growl", "version": "^1.3.1"},
		{"type":"package", "name":"bootstrap-inputmsg", "version": "^1.0.0"},
		{"type":"package", "name":"bootstrap-avatar-preview", "version": "1.1.0"},
		{"type":"package", "name":"dropzone", "version": "^5.5.1"},
		{"type":"package", "name":"bootstrap-tagsinput", "version": "^0.7.1"},
		{"type":"less", "file":"node_modules/bootstrap-avatar-preview/avatarPreview.less"},
		{"type":"less", "file":"assets/less/post.less"},
		{"type":"ts", "file":"assets/ts/Main.ts"}
	],
	"events": [
		{
			"name":"events/initializeDashboard",
			"listener": "listeners/news/dashboard@initialize"
		}
	],
	"views":[
		{
			"name":"views/news/panel/index",
			"parent":"packages/news/views/panel/index",
			"file":"html/post/list.php"
		},
		{
			"name":"views/news/panel/comment",
			"parent":"packages/news/views/panel/comment",
			"file":"html/comment/list.php"
		},
		{
			"name":"views/news/panel/edit",
			"parent":"packages/news/views/panel/edit",
			"file":"html/post/edit.php"
		},
		{
			"name":"views/news/panel/delete",
			"parent":"packages/news/views/panel/delete",
			"file":"html/post/delete.php"
		},
		{
			"name":"views/news/panel/add",
			"parent":"packages/news/views/panel/add",
			"file":"html/post/add.php"
		},
		{
			"name":"views/news/panel/commentDelete",
			"parent":"packages/news/views/panel/commentDelete",
			"file":"html/comment/delete.php"
		},
		{
			"name":"views/news/panel/commentEdit",
			"parent":"packages/news/views/panel/commentEdit",
			"file":"html/comment/edit.php"
		},
		{
			"name":"views/news/panel/view",
			"parent":"packages/news/views/panel/view",
			"file":"html/post/view.php"
		}
	]
}
